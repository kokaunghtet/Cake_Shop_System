<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="270.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="userImage" fitHeight="137.0" fitWidth="162.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </VBox.margin>
      </ImageView>
      <Label fx:id="lblUserName" text="User Name">
         <VBox.margin>
            <Insets left="70.0" />
         </VBox.margin>
      </Label>

      <Label fx:id="lblOrderCount" text="Order Count">
         <VBox.margin>
            <Insets left="65.0" top="10.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cakeshopsystem.controllers.UserViewController">
    <children>
        <VBox alignment="CENTER" layoutX="10.0" layoutY="8.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="TOP_RIGHT">
                    <children>
                        <TextField fx:id="txtSearch" promptText="Search" />
                        <ComboBox fx:id="btnFilter" prefWidth="150.0" promptText="Filter">
                            <HBox.margin>
                                <Insets left="25.0" />
                            </HBox.margin>
                        </ComboBox>
                        <Button fx:id="btnAdd" mnemonicParsing="false" text="Add">
                            <HBox.margin>
                                <Insets left="25.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </HBox>
                <TableView>
                    <columns>
                        <TableColumn fx:id="colName" prefWidth="151.19997882843018" text="Name" />
                        <TableColumn fx:id="colEmail" prefWidth="208.8000030517578" text="Email" />
                        <TableColumn fx:id="colStatue" prefWidth="203.20001220703125" text="Statue" />
                    </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
